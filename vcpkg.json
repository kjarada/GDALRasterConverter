{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/main/scripts/vcpkg.schema.json",
  "name": "your-project-name",
  "version": "1.0.0",
  "dependencies": [
    {
      "name": "qt", // or "qt6" if you prefer Qt 6
      "default-features": false,
      "features": [
        "qtbase",
        "qtwidgets"
        // Add other features if needed, but exclude "qtwebengine"
      ]
    },
    "gdal"
  ],
  "builtin-baseline": "<valid-commit-id>",
  "defaultTriplet": "x64-windows"
}
